<div class='center'>
  <h2><%= I18n.t('devise.sign_up')%></h2>

  <div class='devise_form'>
  <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>

    <div><%= f.label :email, I18n.t('devise.email') %><br />
    <%= f.email_field :email %></div>

    <div><%= f.label :password, I18n.t('devise.password') %><br />
    <%= f.password_field :password %></div>

    <div><%= f.label :password_confirmation, I18n.t('devise.password_confirmation') %><br />
    <%= f.password_field :password_confirmation %></div>

    <div><%= f.submit I18n.t('devise.sign_up') %></div>
  <% end %>

  <%= render :partial => "people/shared/links" %>
  </div>
</div>
